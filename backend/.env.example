# ============================================================================
# Memory Service Configuration (Solder Cortex Integration)
# ============================================================================

# Supabase Connection Pooling
SUPABASE_POOL_MIN=20
SUPABASE_POOL_MAX=100
SUPABASE_CONNECTION_TIMEOUT=30000
SUPABASE_IDLE_TIMEOUT=600000

# Redis Connection Pooling
REDIS_POOL_MIN=10
REDIS_POOL_MAX=50

# LYS Labs WebSocket API
LYS_LABS_WS_URL=wss://api.lyslabs.io/v1/ws
LYS_LABS_API_KEY=your-lys-labs-api-key
LYS_LABS_RECONNECT_ATTEMPTS=5

# Memory Service Cache Configuration
MEMORY_CACHE_TTL=300
MEMORY_CACHE_THRESHOLD=0.8
MEMORY_CACHE_WARMUP=true

# Memory Service Query Configuration
MEMORY_QUERY_PAGE_SIZE=50
MEMORY_QUERY_MAX_PAGE_SIZE=1000
MEMORY_CACHED_QUERY_TIMEOUT=200
MEMORY_SLOW_QUERY_THRESHOLD=1000

# PnL Calculation Configuration
MEMORY_PNL_UPDATE_INTERVAL=10
MEMORY_STALE_PRICE_THRESHOLD=15
MEMORY_COST_BASIS_METHOD=FIFO

# Risk Analysis Configuration
MEMORY_ANOMALY_THRESHOLD=3.0

# Event Emission Configuration
MEMORY_EVENT_RATE_LIMIT=100
MEMORY_EVENT_HEARTBEAT=30

# Circuit Breaker Configuration
MEMORY_CIRCUIT_BREAKER_THRESHOLD=5
MEMORY_CIRCUIT_BREAKER_DURATION=300000

# Retry Configuration
MEMORY_RETRY_MAX_ATTEMPTS=3

# Auto-Registration Configuration
MEMORY_AUTO_REGISTER=true
MEMORY_PROTOCOL_WALLETS=wallet1,wallet2,wallet3

# ============================================================================
# Solana Agent Kit (SAK) Integration Configuration
# ============================================================================

# SAK Core Configuration
SAK_ENABLED=false
SAK_RPC_URL=https://api.devnet.solana.com
SAK_PRIVATE_KEY=your-base64-encoded-private-key
SAK_NETWORK=devnet
SAK_COMMITMENT=confirmed

# SAK Plugin Configuration
SAK_TOKEN_PLUGIN_ENABLED=true
SAK_TOKEN_PLUGIN_PRIORITY=100
SAK_DEFI_PLUGIN_ENABLED=true
SAK_DEFI_PLUGIN_PRIORITY=90
