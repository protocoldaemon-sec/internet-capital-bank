# SAK Integration Settings
SAK_FALLBACK_ENABLED=true
SAK_RETRY_ATTEMPTS=3
SAK_RETRY_DELAY=1000
SAK_TIMEOUT_MS=30000
SAK_BATCH_SIZE=10

# SAK Monitoring Configuration
SAK_METRICS_ENABLED=true
SAK_LOGGING_LEVEL=info
SAK_HEALTH_CHECK_INTERVAL=60000
SAK_ERROR_RATE_THRESHOLD=0.05
SAK_RESPONSE_TIME_THRESHOLD=5000
